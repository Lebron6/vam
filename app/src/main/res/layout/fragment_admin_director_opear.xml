<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

   <RelativeLayout
       android:id="@+id/toolBar"
       android:layout_width="match_parent"
       android:layout_height="@dimen/x100"
       android:background="@color/colorTheme"
       >

      <TextView
          android:id="@+id/toolbar_title"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_centerInParent="true"
          android:layout_gravity="center"
          android:singleLine="true"
          android:text="工作"
          android:textColor="@color/colorWhite"
          android:textSize="@dimen/x36" />

      <ImageButton
          android:id="@+id/image_message"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentRight="true"
          android:layout_centerVertical="true"
          android:layout_marginRight="@dimen/x21"
          android:background="@mipmap/icon_infor" />
   </RelativeLayout>
   <ScrollView
       android:scrollbars="none"
       android:layout_width="match_parent"
       android:layout_height="match_parent">
      <LinearLayout
          android:orientation="vertical"
          android:layout_width="match_parent"
          android:layout_height="match_parent">
         <TextView
             android:paddingLeft="@dimen/x23"
             android:gravity="center_vertical"
             android:textSize="@dimen/x32"
             android:text="通用"
             android:textColor="@color/colorBlack"
             android:layout_marginTop="@dimen/x55"
             android:layout_width="match_parent"
             android:layout_height="@dimen/x45" />

         <LinearLayout
             android:paddingLeft="@dimen/x24"
             android:paddingRight="@dimen/x24"
             android:orientation="horizontal"
             android:layout_marginTop="@dimen/x30"
             android:layout_width="match_parent"
             android:layout_height="@dimen/x200">
            <LinearLayout   android:id="@+id/layout_examination_and_approval"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginRight="@dimen/x9"
                android:background="@drawable/layout_bg_my_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="@dimen/x200">
               <ImageView
                   android:background="@mipmap/icon_exam"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
               <TextView
                   android:layout_marginTop="@dimen/x25"
                   android:text="审批"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x36"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout  android:id="@+id/layout_expense_reimbursement"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/x9"
                android:background="@drawable/layout_bg_my_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
               <ImageView
                   android:background="@mipmap/icon_baoxiao"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
               <TextView
                   android:layout_marginTop="@dimen/x25"
                   android:text="费用报销"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x36"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
            </LinearLayout>
         </LinearLayout>
         <LinearLayout
             android:paddingLeft="@dimen/x24"
             android:paddingRight="@dimen/x24"
             android:orientation="horizontal"
             android:layout_marginTop="@dimen/x20"
             android:layout_width="match_parent"
             android:layout_height="@dimen/x200">
            <LinearLayout
                android:id="@+id/layout_other_departments"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginRight="@dimen/x9"
                android:background="@drawable/layout_bg_my_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
               <ImageView
                   android:background="@mipmap/icon_other"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
               <TextView
                   android:layout_marginTop="@dimen/x25"
                   android:text="其它部门"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x36"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:visibility="invisible"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/x9"
                android:background="@drawable/layout_bg_my_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
               <ImageView
                   android:background="@mipmap/icon_baoxiao"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
               <TextView
                   android:layout_marginTop="@dimen/x25"
                   android:text="费用报销"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x36"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
            </LinearLayout>
         </LinearLayout>
         <TextView
             android:paddingLeft="@dimen/x23"
             android:gravity="center_vertical"
             android:textSize="@dimen/x32"
             android:text="管理员权限"
             android:textColor="@color/colorBlack"
             android:layout_marginTop="@dimen/x55"
             android:layout_width="match_parent"
             android:layout_height="@dimen/x45" />


         <LinearLayout
             android:paddingLeft="@dimen/x24"
             android:paddingRight="@dimen/x24"
             android:orientation="horizontal"
             android:layout_marginTop="@dimen/x30"
             android:layout_width="match_parent"
             android:layout_height="@dimen/x200">
            <LinearLayout
                android:id="@+id/layout_organizational_structure"
                android:gravity="center"
                android:orientation="vertical"
                android:background="@drawable/layout_bg_my_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
               <ImageView
                   android:background="@mipmap/img_org"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
               <TextView
                   android:layout_marginTop="@dimen/x9"
                   android:text="组织架构"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x28"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_new_employee"
                android:layout_marginLeft="@dimen/x20"
                android:layout_marginRight="@dimen/x20"
                android:gravity="center"
                android:orientation="vertical"
                android:background="@drawable/layout_bg_my_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
               <ImageView
                   android:background="@mipmap/img_new"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
               <TextView
                   android:layout_marginTop="@dimen/x9"
                   android:text="添加新员工"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x28"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_departmental_allocation"
                android:gravity="center"
                android:orientation="vertical"
                android:background="@drawable/layout_bg_my_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
               <ImageView
                   android:background="@mipmap/img_fenpei"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />

               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="@dimen/x9"
                   android:text="部门分配设置"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x28"/>
            </LinearLayout>
         </LinearLayout>
         <LinearLayout

             android:paddingLeft="@dimen/x24"
             android:paddingRight="@dimen/x24"
             android:orientation="horizontal"
             android:layout_marginTop="@dimen/x30"
             android:layout_width="match_parent"
             android:layout_height="@dimen/x200">

            <LinearLayout
                android:id="@+id/layout_reimburse_management"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/layout_bg_my_value"
                android:gravity="center"
                android:orientation="vertical">

               <ImageView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:background="@mipmap/img_baoxiao_m" />

               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="@dimen/x9"
                   android:text="报销管理"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x28" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_permissions_allocation"
                android:layout_marginLeft="@dimen/x20"
                android:layout_marginRight="@dimen/x20"
                android:gravity="center"
                android:orientation="vertical"
                android:background="@drawable/layout_bg_my_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
               <ImageView
                   android:background="@mipmap/img_quanxian"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />

               <TextView
                   android:id="@+id/textView2"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="@dimen/x9"
                   android:text="权限分配"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x28"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_annual_distribution_coefficient"
                android:gravity="center"
                android:orientation="vertical"
                android:background="@drawable/layout_bg_my_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
               <ImageView
                   android:background="@mipmap/img_xishu"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />

               <TextView
                   android:id="@+id/textView"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="@dimen/x9"
                   android:text="年度分配系数"
                   android:textColor="@color/colorTheme"
                   android:textSize="@dimen/x28" />
            </LinearLayout>
         </LinearLayout>
         <View
             android:layout_width="match_parent"
             android:layout_height="@dimen/x30"/>
      </LinearLayout>
   </ScrollView>

</LinearLayout>
